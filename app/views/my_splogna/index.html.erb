<h1>My Needs</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Submitted on</th>
  </tr>
  <% @needs.each do |need| %>
    <tr>
      <td><%=link_to need.title, need_url(need) %></td>
      <td><%= need.created_at.to_date%></td>
    </tr>
  <% end %>
</table>
<%= link_to "New Need", new_need_url %>
<h1>My Gots</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Submitted on</th>
  </tr>
  <% @gots.each do |got| %>
    <tr>
      <td><%=link_to got.title, got_url(got) %></td>
      <td><%= got.created_at.to_date%></td>
    </tr>
  <% end %>
</table>
<%= link_to "New Got", new_got_url %>
<h1>My Messages</h1>
<table>
  <tr>
    <th>Subject</th>
    <th>Sent At</th>
  </tr>
  <% @messages.each do |message| %>
    <tr>
      <td><%=link_to message.subject, message_url(message) %></td>
      <td><%= message.created_at.to_date%></td>
    </tr>
  <% end %>
</table>